burger.onclick => () {
    navigation.style.left = 0
    black_bg.style.display="block"

}
cansel.onclick = () => {
    navigation.style.left = "-100%"
    black_bg.style.display = none
}
black_bg.onclick = () => {
    navigation.style.left = "-100%"
    black_bg.style.display = none
}